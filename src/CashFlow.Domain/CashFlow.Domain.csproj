      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>ReportGenerationMessagesResource.resx</DependentUpon>
    </Compile>
    <Compile Update="Reports\ReportPaymentTypeResource.Designer.cs">
      <DesignTime>True</DesignTime>
