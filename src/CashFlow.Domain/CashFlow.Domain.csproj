    <EmbeddedResource Update="Reports\ReportPaymentTypeResource.pt-br.resx">
      <LastGenOutput>ReportPaymentTypeResource.pt-br.Designer.cs</LastGenOutput>
      <Generator>PublicResXFileCodeGenerator</Generator>
